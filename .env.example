# Image Recognition API Keys
# Copy this file to .env and fill in your API keys

# Google Cloud Vision API
# Get your key from: https://console.cloud.google.com/apis/credentials
# Enable: Cloud Vision API
# Free tier: 1,000 units/month
GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Roboflow API
# Get your key from: https://app.roboflow.com/settings/api
# Free tier includes inference credits
ROBOFLOW_API_KEY=your_roboflow_api_key_here

# Clarifai API (Optional)
# Get your key from: https://clarifai.com/settings/security
# Free tier: 1,000 operations/month
CLARIFAI_API_KEY=your_clarifai_api_key_here

# Hugging Face API (Optional)
# Get your token from: https://huggingface.co/settings/tokens
# Free tier available
HUGGINGFACE_API_KEY=your_huggingface_token_here
